{
  "scriptFile": "../shared_code/ChatCompletion_init.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 2,
    "minimumInterval": "00:00:10",
    "maximumInterval": "00:01:30"
  }
}